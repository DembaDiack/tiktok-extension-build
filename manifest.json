{
    "name": "Tiktok autoplay",
    "description": "This extension will play the next tiktok video automaticaly.",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["scripting", "activeTab","storage"],
    "background": {
      "service_worker": "main.js"
    },
    "host_permissions": ["https://www.tiktok.com/*"],
    "action" : {
        "default_icon" : {
          "32" : "svg.png"
        }
    }
  }
  