{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["scripting", "activeTab","storage"],
    "background": {
      "service_worker": "main.js"
    },
    "host_permissions": ["https://www.tiktok.com/*"],
    "action": {
      "default_popup": "index.html"
    }
  }
  